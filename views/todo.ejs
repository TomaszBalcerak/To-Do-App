<html>
	<head>
		<title>Todo App</title>
		<script src="https://code.jquery.com/jquery-1.12.3.min.js"></script>
		<script src="/assets/todo-list.js"></script>
		<link href="/assets/style.css" rel="stylesheet" type="text/css"/>
		<link href="https://fonts.googleapis.com/css?family=Baloo+Bhai|Candal|Chewy&display=swap" rel="stylesheet">
	</head>
	<body>
		<h1>To-Do App</h1>
		<div id="todo-table">
		<form>
			<input type="text" name="item" placeholder="Dodaj nowe zadanie..." required />
			<button type="submit">Dodaj</button>
		</form>
		<ul>
			<% for(var i=0; i < todos.length; i++){ %>
				<li><%= todos[i].item %></li>
			<% } %>
		</ul>
		</div>
</html>